{
  "name": "finterax-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engine": {
    "node": "20.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --build",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register api/index.ts",
    "knex": "knex --knexfile src/config/knexfile.ts",
    "migrate": "npm run knex migrate:latest",
    "seed": "npm run knex seed:run"
  },
  "keywords": [],
  "author": "Moch Fauzi Dwi R",
  "license": "ISC",
  "description": "Beginner Backend FWG 20",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "datauri": "^4.1.0",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "concurrently": "^9.2.1",
